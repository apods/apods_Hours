<% @hours.each do |record| %>
  Event: <%= record.event %><br>
  Hours: <%= record.hours %>
  <% if !record.date.nil? %>
    Date: <%= record.date %><br>
  <% end %>
  <% if !record.area_of_service.nil? %>
    Area of Service: <%= record.area_of_service %>
  <% end %>
  <% if !record.description.nil? %>
    Description: <%= record.description %>
  <% end %><br>
<% end %>
